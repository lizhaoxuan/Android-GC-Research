<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zhaoxuan.bitmaptest">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ByteReplaceBitmapActivity" />
        <activity android:name=".BitmapConfigActivity" />
        <activity android:name=".DecodeTestActivity" />
        <activity android:name=".RecycleBitmapActivity" />
        <activity android:name=".SplashTestActivity" />
        <activity android:name=".OldPhotoActivity" />
        <activity android:name=".DrawableBitmapActivity" />
        <activity android:name=".NewTakePhotoActivity" />
        <activity android:name=".ListViewActivity" />
        <activity android:name=".RecycleListActivity" />
        <activity android:name=".StreamByteActivity" />
        <activity android:name=".LoadSpeedActivity" />
        <activity android:name=".TestChoiceActivity"></activity>
    </application>

</manifest>